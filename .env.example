# Core runtime
AWE_DATABASE_URL=sqlite+pysqlite:///./.agents/runtime/awe-agentcheck.sqlite3
AWE_ARTIFACT_ROOT=.agents
AWE_SERVICE_NAME=awe-agentcheck
AWE_DRY_RUN=false
AWE_WORKFLOW_BACKEND=langgraph
AWE_OTEL_EXPORTER_OTLP_ENDPOINT=

# Provider commands
AWE_CLAUDE_COMMAND=claude -p --dangerously-skip-permissions --strict-mcp-config --effort low --model claude-opus-4-6
AWE_CODEX_COMMAND=codex exec --skip-git-repo-check --dangerously-bypass-approvals-and-sandbox -c model_reasoning_effort=xhigh
AWE_GEMINI_COMMAND=gemini --yolo
AWE_PROVIDER_ADAPTERS_JSON={"qwen":"qwen --model qwen-max"}

# Timeouts and concurrency
AWE_PARTICIPANT_TIMEOUT_SECONDS=3600
AWE_COMMAND_TIMEOUT_SECONDS=300
AWE_PARTICIPANT_TIMEOUT_RETRIES=1
AWE_MAX_CONCURRENT_RUNNING_TASKS=1

# API access policy
AWE_API_ALLOW_REMOTE=false
AWE_API_TOKEN=
AWE_API_TOKEN_HEADER=Authorization

# Promotion/sandbox policy
AWE_PROMOTION_GUARD_ENABLED=true
AWE_PROMOTION_REQUIRE_CLEAN=true
AWE_PROMOTION_ALLOWED_BRANCHES=main,master
AWE_SANDBOX_BASE=
AWE_SANDBOX_USE_PUBLIC_BASE=false

# Architecture audit hard-gate policy
# Mode: off|warn|hard
AWE_ARCH_AUDIT_MODE=hard
AWE_ARCH_PYTHON_FILE_LINES_MAX=1200
AWE_ARCH_FRONTEND_FILE_LINES_MAX=2500
AWE_ARCH_RESPONSIBILITY_KEYWORDS_MAX=10
AWE_ARCH_SERVICE_FILE_LINES_MAX=4500
AWE_ARCH_WORKFLOW_FILE_LINES_MAX=2600
AWE_ARCH_DASHBOARD_JS_LINES_MAX=3800
AWE_ARCH_PROMPT_BUILDER_COUNT_MAX=14
AWE_ARCH_ADAPTER_RUNTIME_RAISE_MAX=0
